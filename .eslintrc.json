{
  "env": {
    "es6": true,
    "jest": true,
    "browser": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "airbnb-base/legacy",
    "plugin:prettier/recommended"
  ],
  "rules": {},
  "overrides": [
    {
      "files": [
        "*.js"
      ],
      "rules": {
        "strict": "off"
      }
    }
  ]
}
